{
  "name": "myfile",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "коментарий": "скрипт который переводит scss в css",
    "scss-compile": "sass --style expanded --source-map --embed-sources --no-error-css scss/:css/",
    "коментарий1": "скрипт который автоматически запускает scss-compile ",
    "watch-scss": "nodemon --watch scss/ --ext scss --exec \"npm-run-all scss-compile\"",
    "start": "node ./config/esbuild-dev.js",
    "build:prod": "node ./config/esbuild-prod.js"
  },
  "author": "Ustim",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-sass": "^3.4.3",
    "esbuild": "^0.14.48",
    "esbuild-plugin-sass": "^1.0.1",
    "express": "^4.18.1",
    "jquery": "^3.6.0",
    "nodemon": "^2.0.16",
    "sass": "^1.52.2",
    "swiper": "^7.4.1"
  }
}
